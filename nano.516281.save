int check_philo_death(t_data *data, int i)
{
	int time_since_meal;
	
	pthread_mutex_lock(&data->monitor_mutex);
	time_since_meal = get_time_ms()
		- data->philosophers[i].last_meal_time;
	pthread_mutex_unlock(&data->monitor_mutex);
	if (time_since_meal > data->time_to_die)
	{
		print_locked(&data->philosophers[i], "died");
		return (1);
	}
	return (0);
}
